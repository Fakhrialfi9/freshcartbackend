{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "notebook.formatOnSave.enabled": true,
  "prettier.printWidth": 150,
  "files.autoSave": "onFocusChange",
  "explorer.confirmDelete": false,
  "workbench.colorTheme": "One Dark Pro Darker",
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "on",
  "workbench.iconTheme": "lucodear-icons",
  "window.zoomLevel": 1,
  "security.workspace.trust.untrustedFiles": "open",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "notebook.formatOnCellExecution": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "security.promptForLocalFileProtocolHandling": false,
  "bladeFormatter.format.sortTailwindcssClasses": true,
  "bladeFormatter.format.noPhpSyntaxCheck": true,
  "bladeFormatter.misc.dontShowNewVersionMessage": true,
  "editor.formatOnSave": true,
  "bladeFormatter.formatOnSave": true,
  "bladeFormatter.formatOnSaveEnabled": true,
  "bladeFormatter.format.indentInnerHtml": true,
  "[blade]": {
    "editor.defaultFormatter": "shufo.vscode-blade-formatter"
  },
  "indentSize": 4,
  "wrapAttributes": "auto",
  "wrapAttributesMinAttrs": 2,
  "wrapLineLength": 120,
  "endWithNewLine": true,
  "noMultipleEmptyLines": false,
  "useTabs": false,
  "sortTailwindcssClasses": true,
  "sortHtmlAttributes": "none",
  "noPhpSyntaxCheck": false,
  "noSingleQuote": false,
  "auto-close-tag.fullMode": true,
  "auto-close-tag.SublimeText3Mode": true,
  "auto-close-tag.insertSpaceBeforeSelfClosingTag": true,
  "auto-close-tag.activationOnLanguage": [
    "xml",
    "php",
    "blade",
    "ejs",
    "jinja",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "plaintext",
    "markdown",
    "vue",
    "liquid",
    "erb",
    "lang-cfml",
    "cfml",
    "HTML (EEx)",
    "HTML (Eex)",
    "plist"
  ],
  "auto-close-tag.excludedTags": [
    "area",
    "base",
    "br",
    "col",
    "command",
    "embed",
    "hr",
    "img",
    "input",
    "keygen",
    "link",
    "meta",
    "param",
    "source",
    "track",
    "wbr"
  ],
  "prettier.bracketSameLine": true,
  "prettier.enableDebugLogs": true,
  "prettier.trailingComma": "all",
  "prettier.vueIndentScriptAndStyle": true,
  "prettier.withNodeModules": true,
  "prettier.jsxSingleQuote": true,
  "editor.codeActionsOnSave": {},
  "editor.wordWrapColumn": 150,
  "sonarlint.rules": {
    "Web:ImgWithoutAltCheck": {
      "level": "off"
    },
    "Web:S6851": {
      "level": "off"
    },
    "Web:AvoidCommentedOutCodeCheck": {
      "level": "off"
    },
    "typescript:S878": {
      "level": "off"
    }
  },
  "extensions.ignoreRecommendations": true,
  "vue.format.script.initialIndent": true,
  "vue.inlayHints.inlineHandlerLeading": true,
  "vue.inlayHints.vBindShorthand": true,
  "vue.inlayHints.optionsWrapper": true,
  "vue.inlayHints.missingProps": true,
  "vue.splitEditors.icon": true,
  "vue.trace.server": "messages",
  "diffEditor.ignoreTrimWhitespace": false,
  "vue.format.style.initialIndent": true,
  "vue.autoInsert.dotValue": true,
  "vue.server.hybridMode": true,
  "importCost.debug": true,
  "path-intellisense.autoSlashAfterDirectory": true,
  "path-intellisense.autoTriggerNextSuggestion": true,
  "path-intellisense.ignoreTsConfigBaseUrl": true,
  "path-intellisense.extensionOnImport": true,
  "path-intellisense.showHiddenFiles": true,
  "path-intellisense.mappings": {},
  "path-intellisense.showOnAbsoluteSlash": false,
  "workbench.settings.applyToAllProfiles": ["path-intellisense.absolutePathTo"],
  "extensions.autoUpdate": "onlyEnabledExtensions"
}
